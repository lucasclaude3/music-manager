{
  "name": "music-manager",
  "version": "0.0.1",
  "author": "kahlua <lucasclaude3@gmail.com>",
  "description": "An Electron-Vue project for managing your music easily",
  "license": null,
  "main": "./dist/electron/main.js",
  "scripts": {
    "build": "node .electron-vue/build.js && electron-builder",
    "build:dir": "node .electron-vue/build.js && electron-builder --dir",
    "build:clean": "cross-env BUILD_TARGET=clean node .electron-vue/build.js",
    "build:web": "cross-env BUILD_TARGET=web node .electron-vue/build.js",
    "dev": "node .electron-vue/dev-runner.js",
    "e2e": "yarn run pack && mocha test/e2e",
    "lint": "eslint --ext .js,.vue -f ./node_modules/eslint-friendly-formatter src test",
    "lint:fix": "eslint --ext .js,.vue -f ./node_modules/eslint-friendly-formatter --fix src test",
    "pack": "yarn run pack:main && yarn run pack:renderer",
    "pack:main": "cross-env NODE_ENV=production webpack --progress --colors --config .electron-vue/webpack.main.config.js",
    "pack:renderer": "cross-env NODE_ENV=production webpack --progress --colors --config .electron-vue/webpack.renderer.config.js",
    "test": "yarn run unit && yarn run e2e",
    "unit": "karma start test/unit/karma.conf.js",
    "postinstall": "yarn run lint:fix",
    "generate-icons": "vsvg -s ./src/renderer/assets/svg-icons -t ./src/renderer/assets/compiled-icons"
  },
  "build": {
    "productName": "music-manager",
    "appId": "com.example.yourapp",
    "directories": {
      "output": "build"
    },
    "files": [
      "dist/electron/**/*"
    ],
    "dmg": {
      "contents": [
        {
          "x": 410,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        },
        {
          "x": 130,
          "y": 150,
          "type": "file"
        }
      ]
    },
    "mac": {
      "icon": "build/icons/icon.icns"
    },
    "win": {
      "icon": "build/icons/icon.ico"
    },
    "linux": {
      "icon": "build/icons"
    }
  },
  "dependencies": {
    "axios": "^0.18.0",
    "bluebird": "^3.7.2",
    "bootstrap": "^4.4.1",
    "bootstrap-vue": "^2.1.0",
    "bulksearch": "^0.1.3",
    "dataurl": "^0.1.0",
    "dotenv": "^8.2.0",
    "electron": "^7.2.4",
    "electron-store": "^5.1.0",
    "fs": "^0.0.1-security",
    "howler": "^2.1.3",
    "mime-db": "^1.44.0",
    "mime-type": "^3.0.7",
    "node-id3": "^0.1.14",
    "terser-webpack-plugin": "^2.3.1",
    "uuid": "^8.3.2",
    "vue": "^2.5.16",
    "vue-electron": "^1.0.6",
    "vue-js-modal": "^2.0.0-rc.3",
    "vue-router": "^3.0.1",
    "vue-svgicon": "^3.2.6",
    "vue-youtube": "^1.4.0",
    "vuex": "^3.0.1",
    "vuex-electron": "^1.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.7.2",
    "ajv": "^6.12.3",
    "babel-eslint": "^8.2.3",
    "babel-loader": "^7.1.4",
    "babel-plugin-istanbul": "^4.1.6",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.26.0",
    "babili-webpack-plugin": "^0.1.2",
    "cfonts": "^2.1.2",
    "chai": "^4.1.2",
    "chalk": "^2.4.1",
    "copy-webpack-plugin": "^5.1.1",
    "cross-env": "^5.1.6",
    "css-loader": "^3.4.0",
    "del": "^3.0.0",
    "devtron": "^1.4.0",
    "electron-builder": "^21.2.0",
    "electron-debug": "^1.5.0",
    "electron-devtools-installer": "^2.2.4",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-import-resolver-webpack": "^0.10.0",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-html": "^6.0.0",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-vue": "^6.0.1",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "^3.2.0",
    "inject-loader": "^4.0.1",
    "karma": "^4.4.1",
    "karma-chai": "^0.1.0",
    "karma-coverage": "^2.0.1",
    "karma-electron": "^6.0.0",
    "karma-mocha": "ssh://git@github.com:lucasclaude3/karma-mocha",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-spec-reporter": "^0.0.32",
    "karma-webpack": "^3.0.0",
    "mini-css-extract-plugin": "0.4.0",
    "mocha": "^5.2.0",
    "node-loader": "^0.6.0",
    "node-sass": "^4.13.1",
    "ora": "^4.0.3",
    "require-dir": "^1.0.0",
    "sass-loader": "^7.0.3",
    "spectron": "9.0.0",
    "style-loader": "^0.21.0",
    "url-loader": "^1.0.1",
    "vue-html-loader": "^1.2.4",
    "vue-loader": "^15.2.4",
    "vue-style-loader": "^4.1.0",
    "vue-template-compiler": "^2.5.16",
    "webpack": "^4.41.4",
    "webpack-cli": "^3.0.8",
    "webpack-dev-server": "^3.1.4",
    "webpack-hot-middleware": "^2.22.2",
    "webpack-merge": "^4.1.3"
  }
}
